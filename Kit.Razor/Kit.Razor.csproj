    <LangVersion>latest</LangVersion>
    <RootNamespace>Kit.Razor</RootNamespace>
    <AssemblyName>Kit.Razor</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="3.1.19" />
